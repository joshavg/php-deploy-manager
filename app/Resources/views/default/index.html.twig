{% extends 'base.html.twig' %}

{% block body %}
    <h1>{{ 'title.deploy.application'|trans }}</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>{{ 'th.app.name'|trans }}</th>
            </tr>
        </thead>
        <tbody>
            {% for app in apps %}
                <tr>
                    <td>{{ app.name }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
